{
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios --udid='296161BC-BD76-48D6-8AED-F29B785EB8C6'",
    "web": "expo start --web",
    "lint:write": "eslint src/ --fix",
    "prettier": "prettier --write src/**/*.js",
    "clean": "yarn reinstall:modules && yarn clean:android && yarn clean:ios",
    "reinstall:modules": "rm -rf node_modules && yarn",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && pod install && cd .."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "yarn lint:write",
      "git add"
    ]
  },
